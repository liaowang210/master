<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.SearchMessageMapper">
    <select id="selectByScore" resultType="com.example.demo.entity.Search">
        select *
        from search_message
        where score=#{score}
    </select>
</mapper>