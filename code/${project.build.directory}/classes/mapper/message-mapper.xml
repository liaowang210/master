<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.code.dao.SearchMessageMapper">
    <select id="selectByKeyword" resultType="Search">
        select search_message1,search_message2,score,keyword
        from search_message
        where score=#{score}
    </select>
</mapper>